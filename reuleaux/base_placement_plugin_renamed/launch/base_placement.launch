<?xml version="1.0"?>
<launch>

<arg name = "moveit_pkg" default="tiago_moveit_config"/>

<!--Launch moveit required pkgs for your robot-->
<include file="$(find base_placement_plugin_renamed)/launch/moveit_pkgs.launch">
    <arg name="moveit_pkg" value="$(arg moveit_pkg)"/>  
    
</include>

<!--Running RVIZ for base_placement-->
<node pkg="rviz" type="rviz" output="screen" name="rviz" args="-d $(find base_placement_plugin_renamed)/rviz/base_placement.rviz" />


</launch>
